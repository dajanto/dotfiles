set encoding=utf-8
set background=dark
set t_Co=256
colorscheme monokai
syntax enable
set fileencoding=utf-8
:set number
set autoindent
set spell
set spelllang=de
set noexpandtab
set tabstop=4
set shiftwidth=4
set relativenumber
set hlsearch
map <f5> :mksession! session.vim
map <f6> :source session.vim
